{
    "targetRepository": "your-github-username/your-repository-name",
    "draftDirectory": "draft",
    "content": [
        {
            "directory": "posts",
            "articleFile": "index.md",
            "imageDirInsideContent": true,
            "metaCache": {
                "type": "json",
                "path": "posts/index.json"
            }
        },
        {
            "directory": "memo",
            "articleFile": "memo.md",
            "imageDirInsideContent": true,
            "metaCache": {
                "type": "json",
                "path": "memo/index.json"
            }
        }
    ],
    "webhooks": {
        "enabled": true,
        "endpoints": [
            {
                "name": "vercel-deploy",
                "url": "https://api.vercel.com/v1/integrations/deploy/your-deploy-hook-url",
                "events": ["create", "update", "delete"]
            }
        ]
    }
}
