AUTH_SECRET=your-auth-secret
AUTH_GITHUB_ID=your-github-client-id
AUTH_GITHUB_SECRET=your-github-client-secret
# ALLOWED_USERS supports plain values (match login/email/id/name)
# or explicit prefixes: login:<github-login>, email:<address>, id:<github-id>, name:<display-name>
ALLOWED_USERS=login:your-github-login,email:you@example.com

# optional
GOOGLE_CREDENTIALS_JSON=your-google-credentials-json
GOOGLE_VERTEX_PROJECT_ID=your-gcp-project-id
GOOGLE_VERTEX_LOCATION=your-gcp-location
WEBHOOK_SECRET=your-webhook-secret-here
